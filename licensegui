    -- Ð¤ÑƒÐ½ÐºÑ†Ð¸Ñ Ð²ÐµÑ€Ð¸Ñ„Ð¸ÐºÐ°Ñ†Ð¸Ð¸
    local function verifyKey()
        local key = keyBox.Text
        if key == "" or #key < 10 then
            statusLabel.Text = "âš  Please enter a valid license key"
            statusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
            
            local originalPos = authFrame.Position
            for i = 1, 4 do
                TweenService:Create(authFrame, TweenInfo.new(0.04), {Position = originalPos + UDim2.new(0, 12, 0, 0)}):Play()
                wait(0.04)
                TweenService:Create(authFrame, TweenInfo.new(0.04), {Position = originalPos - UDim2.new(0, 12, 0, 0)}):Play()
                wait(0.04)
            end
            authFrame.Position = originalPos
            return
        end
        
        submitBtn.Text = "VERIFYING..."
        submitBtn.BackgroundColor3 = Color3.fromRGB(140, 100, 190)
        statusLabel.TextColor3 = Color3.fromRGB(220, 180, 250)
        
        local loading = true
        spawn(function()
            local dotCount = 0
            while loading do
                statusLabel.Text = "Verifying license" .. string.rep(".", (dotCount % 4))
                dotCount = dotCount + 1
                wait(0.4)
            end
        end)
        
        local verifySuccess, success, message = pcall(function()
            return verifyLicense(key)
        end)
        
        loading = false
        
        if verifySuccess and success then
            isAuthorized = true
            statusLabel.Text = "âœ“ " .. message
            statusLabel.TextColor3 = Color3.fromRGB(0, 255, 150)
            
            -- ÐÐ½Ð¸Ð¼Ð°Ñ†Ð¸Ñ ÑƒÑÐ¿ÐµÑ…Ð° - Ð»Ð¾Ð³Ð¾Ñ‚Ð¸Ð¿ ÑÑ‚Ð°Ð½Ð¾Ð²Ð¸Ñ‚ÑÑ Ð·ÐµÐ»ÐµÐ½Ñ‹Ð¼ Ñ Ð³Ð°Ð»Ð¾Ñ‡ÐºÐ¾Ð¹
            logoText.Text = "âœ“"
            TweenService:Create(logoText, TweenInfo.new(0.6, Enum.EasingStyle.Back), {
                TextSize = 75,
                TextColor3 = Color3.fromRGB(255, 255, 255)
            }):Play()
            
            TweenService:Create(logoFrame, TweenInfo.new(0.6, Enum.EasingStyle.Back), {
                BackgroundColor3 = Color3.fromRGB(0, 220, 120),
                Rotation = 0
            }):Play()
            
            TweenService:Create(logoGradient, TweenInfo.new(0.6), {
                Rotation = 0
            }):Play()
            logoGradient.Color = ColorSequence.new{
                ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 255, 150)),
                ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 200, 100))
            }
            
            TweenService:Create(logoStroke, TweenInfo.new(0.6), {
                Color = Color3.fromRGB(100, 255, 180),
                Transparency = 0
            }):Play()
            
            TweenService:Create(submitBtn, TweenInfo.new(0.6), {
                BackgroundColor3 = Color3.fromRGB(0, 220, 120)
            }):Play()
            
            submitBtn.Text = "âœ“ VERIFIED"
            
            wait(2.5)
            
            particleConnection:Disconnect()
            
            -- ÐÐ½Ð¸Ð¼Ð°Ñ†Ð¸Ñ Ð¸ÑÑ‡ÐµÐ·Ð½Ð¾Ð²ÐµÐ½Ð¸Ñ
            TweenService:Create(authFrame, TweenInfo.new(0.9, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
                Size = UDim2.new(0, 0, 0, 0),
                Position = UDim2.new(0.5, 0, 0.5, 0),
                BackgroundTransparency = 1,
                Rotation = 180
            }):Play()
            
            TweenService:Create(darkBackground, TweenInfo.new(0.9), {
                BackgroundTransparency = 1
            }):Play()
            
            for _, particle in ipairs(particles) do
                TweenService:Create(particle.frame, TweenInfo.new(0.9), {
                    BackgroundTransparency = 1
                }):Play()
            end
            
            wait(0.9)
            authGui:Destroy()
            initializeScript()
        else
            submitBtn.Text = "VERIFY LICENSE"
            submitBtn.BackgroundColor3 = Color3.fromRGB(120, 0, 220)
            statusLabel.Text = "âœ— " .. (message or "Verification failed")
            statusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
            
            -- ÐÐ½Ð¸Ð¼Ð°Ñ†Ð¸Ñ Ð¾ÑˆÐ¸Ð±ÐºÐ¸
            TweenService:Create(logoFrame, TweenInfo.new(0.3), {
                BackgroundColor3 = Color3.fromRGB(220, 0, 80)
            }):Play()
            wait(0.5)
            TweenService:Create(logoFrame, TweenInfo.new(0.5), {
                BackgroundColor3 = Color3.fromRGB(100, 0, 200)
            }):Play()
            
            local originalPos = authFrame.Position
            for i = 1, 4 do
                TweenService:Create(authFrame, TweenInfo.new(0.04), {Position = originalPos + UDim2.new(0, 12, 0, 0)}):Play()
                wait(0.04)
                TweenService:Create(authFrame, TweenInfo.new(0.04), {Position = originalPos - UDim2.new(0, 12, 0, 0)}):Play()
                wait(0.04)
            end
            authFrame.Position = originalPos
        end
    end
    
    submitBtn.MouseButton1Click:Connect(verifyKey)
    keyBox.FocusLost:Connect(function(enterPressed)
        if enterPressed then
            verifyKey()
        end
    end)
    
    -- Hover ÑÑ„Ñ„ÐµÐºÑ‚Ñ‹
    submitBtn.MouseEnter:Connect(function()
        TweenService:Create(submitBtn, TweenInfo.new(0.2), {
            Size = UDim2.new(0, 250, 0, 60),
            BackgroundColor3 = Color3.fromRGB(140, 0, 240)
        }):Play()
        TweenService:Create(submitBtnStroke, TweenInfo.new(0.2), {
            Transparency = 0
        }):Play()
    end)
    
    submitBtn.MouseLeave:Connect(function()
        if submitBtn.Text == "VERIFY LICENSE" then
            TweenService:Create(submitBtn, TweenInfo.new(0.2), {
                Size = UDim2.new(0, 240, 0, 56),
                BackgroundColor3 = Color3.fromRGB(120, 0, 220)
            }):Play()
            TweenService:Create(submitBtnStroke, TweenInfo.new(0.2), {
                Transparency = 0.3
            }):Play()
        end
    end)
    
    getKeyBtn.MouseEnter:Connect(function()
        TweenService:Create(getKeyBtn, TweenInfo.new(0.2), {
            Size = UDim2.new(0, 210, 0, 38),
            BackgroundColor3 = Color3.fromRGB(90, 50, 140)
        }):Play()
        TweenService:Create(getKeyStroke, TweenInfo.new(0.2), {
            Transparency = 0.2
        }):Play()
    end)
    
    getKeyBtn.MouseLeave:Connect(function()
        TweenService:Create(getKeyBtn, TweenInfo.new(0.2), {
            Size = UDim2.new(0, 200, 0, 36),
            BackgroundColor3 = Color3.fromRGB(70, 35, 110)
        }):Play()
        TweenService:Create(getKeyStroke, TweenInfo.new(0.2), {
            Transparency = 0.5
        }):Play()
    end)
    
    -- ÐÐ½Ð¸Ð¼Ð°Ñ†Ð¸Ñ Ð¿Ð¾ÑÐ²Ð»ÐµÐ½Ð¸Ñ
    authFrame.Size = UDim2.new(0, 0, 0, 0)
    authFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
    authFrame.BackgroundTransparency = 1
    authFrame.Rotation = -180
    
    TweenService:Create(authFrame, TweenInfo.new(1.2, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
        Size = UDim2.new(0, 540, 0, 520),
        Position = UDim2.new(0.5, -270, 0.5, -260),
        BackgroundTransparency = 0.02,
        Rotation = 0
    }):Play()
end

-- WalkSpeed ÑÐ¸ÑÑ‚ÐµÐ¼Ð°
getgenv().walkSpeedSettings = {
    WalkSpeed = {
        Enabled = false,
        Speed = 300,
    },
    Activation = {
        WalkSpeedToggleKey = "Q",
    }
}

local isSpeedEnabled = false
local originalSpeeds = {}
local speedConnection = nil

local function saveOriginalSpeed()
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        local humanoid = LocalPlayer.Character.Humanoid
        if not originalSpeeds[humanoid] then
            originalSpeeds[humanoid] = humanoid.WalkSpeed
        end
    end
end

local function updateSpeed()
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        local humanoid = LocalPlayer.Character.Humanoid
        
        if isSpeedEnabled and getgenv().walkSpeedSettings.WalkSpeed.Enabled then
            humanoid.WalkSpeed = getgenv().walkSpeedSettings.WalkSpeed.Speed
        else
            if originalSpeeds[humanoid] then
                humanoid.WalkSpeed = originalSpeeds[humanoid]
            end
        end
    end
end

local function setupWalkSpeed()
    if speedConnection then
        speedConnection:Disconnect()
    end
    
    LocalPlayer.CharacterAdded:Connect(function(character)
        character:WaitForChild("Humanoid")
        wait(0.1)
        saveOriginalSpeed()
        updateSpeed()
    end)
    
    saveOriginalSpeed()
    
    speedConnection = RunService.Heartbeat:Connect(function()
        if isSpeedEnabled and getgenv().walkSpeedSettings.WalkSpeed.Enabled then
            updateSpeed()
        end
    end)
end

function initializeScript()
    if not isAuthorized then
        return
    end

