-- Ð£Ð²ÐµÐ´Ð¾Ð¼Ð»ÐµÐ½Ð¸Ðµ Ð¾ Ð·Ð°Ð³Ñ€ÑƒÐ·ÐºÐµ
local notificationGui = Instance.new("ScreenGui")
notificationGui.IgnoreGuiInset = true
notificationGui.ResetOnSpawn = false
notificationGui.Parent = CoreGui

local notification = Instance.new("Frame")
notification.Size = UDim2.new(0, 420, 0, 110)
notification.Position = UDim2.new(0.5, -210, 0, -130)
notification.BackgroundColor3 = Color3.fromRGB(25, 15, 40)
notification.BorderSizePixel = 0
notification.Parent = notificationGui

local notifCorner = Instance.new("UICorner", notification)
notifCorner.CornerRadius = UDim.new(0, 14)

local notifStroke = Instance.new("UIStroke", notification)
notifStroke.Color = Color3.fromRGB(150, 0, 255)
notifStroke.Thickness = 2

local notifGradient = Instance.new("UIGradient", notification)
notifGradient.Color = ColorSequence.new{
    ColorSequenceKeypoint.new(0, Color3.fromRGB(50, 30, 70)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(35, 20, 55))
}

local notifTitle = Instance.new("TextLabel")
notifTitle.Size = UDim2.new(1, 0, 0, 45)
notifTitle.Position = UDim2.new(0, 0, 0, 18)
notifTitle.Text = "FENNIXXX"
notifTitle.TextColor3 = Color3.fromRGB(150, 0, 255)
notifTitle.TextSize = 20
notifTitle.Font = Enum.Font.GothamBold
notifTitle.BackgroundTransparency = 1
notifTitle.Parent = notification

local notifText = Instance.new("TextLabel")
notifText.Size = UDim2.new(1, 0, 0, 32)
notifText.Position = UDim2.new(0, 0, 0, 62)
notifText.Text = "Press INSERT to open menu"
notifText.TextColor3 = Color3.fromRGB(200, 150, 255)
notifText.TextSize = 14
notifText.Font = Enum.Font.Gotham
notifText.BackgroundTransparency = 1
notifText.Parent = notification

TweenService:Create(notification, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
    Position = UDim2.new(0.5, -210, 0, 25)
}):Play()

wait(3)

TweenService:Create(notification, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
    Position = UDim2.new(0.5, -210, 0, -130)
}):Play()

wait(0.5)
notificationGui:Destroy()

print("FENNIXXX Script Loaded Successfully!")
print("Press", Config.Keybind.Name, "to toggle Silent Aim")
print("Press", getgenv().walkSpeedSettings.Activation.WalkSpeedToggleKey, "to toggle WalkSpeed")
print("Press INSERT to toggle GUI")
end

